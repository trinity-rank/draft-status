{
    "name": "trinityrank/draft-status",
    "description": "Draft status package.",
    "keywords": [
        "laravel",
        "nova",
        "draft",
        "publish"
    ],
    "license": "MIT",
    "require": {
        "php": "^8.0|^7.3",
        "laravel/nova": "^3.0"
    },
    "autoload": {
        "psr-4": {
            "Trinityrank\\DraftStatus\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Trinityrank\\DraftStatus\\FieldServiceProvider"
            ]
        }
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
